{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{jsx,js,tsx,ts}\"",
    "preview": "vite preview",
    "backend": "cd ../backend && poetry run uvicorn src.main:app",
    "webhook": "cd src/api && node webhook-server.cjs",
    "gui": "npm run dev",
    "gui-serve": "serve -s dist -l 5173",
    "start-all": "concurrently -k -n BACKEND,WEBHOOK,GUI -c yellow,cyan,green \"npm run backend\" \"npm run webhook\" \"npm run gui\"",
    "start-all-prod": "npm run build && concurrently -k -n BACKEND,WEBHOOK,GUI -c yellow,cyan,green \"npm run backend\" \"npm run webhook\" \"npm run gui-serve\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource/inter": "^5.2.5",
    "@fontsource/playfair-display": "^5.2.5",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-icons": "^1.3.2",
    "autoprefixer": "^10.4.21",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^12.7.4",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.3",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "uuid": "^11.1.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.1"
  }
}
